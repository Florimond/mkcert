{
  "name": "mkcert",
  "version": "0.0.2",
  "description": "Create Self Signed Development Certificates",
  "main": "lib/mkcert.js",
  "bin": {
    "mkcert": "./lib/cli.js"
  },
  "scripts": {
    "test": "npm run build && jest",
    "build": "npm run clean-lib && npm run compile-src",
    "compile-src": "babel src --out-dir lib",
    "clean-lib": "del-cli ./lib",
    "prepare": "npm run build && npm run test"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "fixtures"
    ]
  },
  "babel": {
    "plugins": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Subash/mkcert.git"
  },
  "keywords": [
    "signed",
    "certificate",
    "cert"
  ],
  "author": "Subash Pathak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Subash/mkcert/issues"
  },
  "homepage": "https://github.com/Subash/mkcert#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "del-cli": "^1.1.0",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "is-ip": "^2.0.0",
    "node-forge": "^0.7.6",
    "random-int": "^1.0.0"
  }
}
